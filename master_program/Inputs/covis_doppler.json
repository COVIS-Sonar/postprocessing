{
    "type": "doppler",
    "comments": "ASHES at Axial",

    "user": {
        "verbose": 1,
        "debug": 0,
        "view": {
            "azimuth": 0.0,
            "elevation": 0.0
        },
        "outpath": "nan"
    },

    "sonar": {
        "position": {
            "easting": 0,
            "northing": 0,
            "depth": -1500,
            "altitude": 4.2,
            "declination": 16.0,
            "heading": 0
        }
    },

    "processing": {
        "beamformer": {
           "type": "fast"
        },
        "calibrate": {
           "mode": "VSS"
        },
        "filter": {
           "status": "on",
           "type": "butterworth",
           "bw": 4,
           "order": 4,
           "decimation": 1        },
        "ping_combination": {
           "mode": "diff"
        },
        "correlation": {
           "window_size": 0.001,
           "window_overlap": 0.5,
           "windthresh": 1.0e-9,
           "nlag": 8
        },
        "bounds": {
            "pitch": {
                "start": 0,
                "stop": 60
            },
            "heading": {
                "start": 0,
                "stop": 0
            },
            "range": {
                "start": 0,
                "stop": 0
            }
        }
    },

    "grid": [
      {
        "type": "doppler velocity", 
        "shape": "rectangular", 
        "units": {
           "spatial": "meters",
           "value": "cm/second"
        },
        "dimensions": 3,
        "bounds": {
            "xmin": -40, "xmax": 20,
            "ymin": -40, "ymax": 20,
            "zmin":  0, "zmax":  20
        },
        "spacing": {
            "dx": 0.5, "dy": 0.5, "dz": 0.5
        }
      },
      {
        "type": "intensity", 
        "shape": "rectangular", 
        "units": {
           "spatial": "meters",
           "value": "db"
        },
        "dimensions": 3,
        "bounds": {
            "xmin": -40, "xmax": 20,
            "ymin": -40, "ymax": 20,
            "zmin":  0, "zmax":  20
        },
        "spacing": {
            "dx": 0.5, "dy": 0.5, "dz": 0.5
        }
      },
      {
        "type": "intensity_win", 
        "shape": "rectangular", 
        "units": {
           "spatial": "meters",
           "value": "db"
        },
        "dimensions": 3,
        "bounds": {
            "xmin": -40, "xmax": 20,
            "ymin": -40, "ymax": 20,
            "zmin":  0, "zmax":  20
        },
        "spacing": {
            "dx": 0.5, "dy": 0.5, "dz": 0.5
        }
      }
   ]

}
